<MetroControls:MetroWindow x:Class="TestProject_Versioning.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:MetroControls="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:local="clr-namespace:TestProject_Versioning"
        xmlns:infoHelper="clr-namespace:AssemblyInfoHelper;assembly=AssemblyInfoHelper"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <MetroControls:MetroWindow.RightWindowCommands>
        <MetroControls:WindowCommands>
            <infoHelper:AppInfoButton EnableNewVersionNotification="True"/>
        </MetroControls:WindowCommands>
    </MetroControls:MetroWindow.RightWindowCommands>

    <Grid>
    </Grid>
</MetroControls:MetroWindow>
